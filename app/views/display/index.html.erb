<h1>All Posts</h1>
<table>
  <thead>
    <th>Title</th>
    <th>Description</th>
    <th>Image</th>
  </thead>
  <% @blogs.each do |blog| %>
     <tr>
       <td><%= blog.title %></td>
       <td><%= blog.description %></td>
       <td><%= link_to image_tag(blog.image), blog_path(blog) %></td>
     </tr>

  <% end %>
</table>
